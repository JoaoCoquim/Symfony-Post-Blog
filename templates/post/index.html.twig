{% extends 'base.html.twig' %}

{% block title %}My Posts!{% endblock %}

{% block body %}
    <h1>List of all posts</h1>

    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
        </tr>
        </thead>
        <tbody>
        {% for post in posts %}
        <tr>
            <th scope="row">{{ post.id }}</th>
            <td>{{ post.title }}</td>
        </tr>
        </tbody>
        {% endfor %}
    </table>


{% endblock %}
